{
    "functions": {
      "app/api/**/*.js": {
        "maxDuration": 10
      }
    },
    "headers": [
      {
        "source": "/obs/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors *;"
          }
        ]
      }
    ]
  }